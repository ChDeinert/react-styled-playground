{
  "name": "react-styled-playground",
  "version": "1.1.0",
  "description": "A playground to try out different ways to style React Components.",
  "scripts": {
    "start": "start-storybook -p 8080",
    "lint": "eslint --ext .js,.jsx src",
    "lint:fix": "eslint --ext .js,.jsx src --fix",
    "gh-pages:clean": "rimraf _gh-pages",
    "gh-pages:build": "build-storybook -o _gh-pages",
    "gh-pages:publish": "git-directory-deploy --directory _gh-pages",
    "gh-pages": "npm run gh-pages:clean && npm run gh-pages:build && npm run gh-pages:publish"
  },
  "author": "ChDeinert <ChDeinert@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:ChDeinert/react-styled-playground.git"
  },
  "devDependencies": {
    "eslint": "^4.6.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "git-directory-deploy": "^1.5.1"
  },
  "dependencies": {
    "@storybook/react": "^3.2.8",
    "glamor": "^2.20.40",
    "glamorous": "^4.9.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "styled-components": "^2.1.2"
  }
}
